{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build JAsioServer: All (Debug x64)",
      "type": "shell",
      "command": "powershell",
      "args": [
        "-NoProfile",
        "-ExecutionPolicy",
        "Bypass",
        "-File",
        ".vscode/build-All.ps1"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": "$msCompile",
      "group": "build"
    }
  ]
}
